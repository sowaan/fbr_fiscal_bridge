 "css": ".greetings {\n    border: 2px solid black;\n    margin: 5px 40px 5px 40px;\n    text-align: center;\n    padding: 2px 10px 2px 10px;\n}\n\n.reduced-line-height {\n    line-height: 0.5 !important;\n}\n.table thead th {\n    color: black; /* Change this to your desired color */\n}\n\n.print-format table, .print-format tr, \n.print-format td, .print-format div, .print-format p {\n    font-family: Monospace;\n    vertical-align: middle;\n}\n@media screen {\n    .print-format {\n        width: 4in;\n        padding: 0.25in;\n        min-height: 8in;\n    }\n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Sales Invoice",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<style>\n\n</style>\n\n<p class=\"text-center\">\n    <div style=\"text-align: center;\">\n        <img src=\"/files/logo.jpg\" style=\"width: 50%;\">\n    </div>\n    <!--<h4> <b><center>Sales Invoice</center></b></h4><br>-->\n</p>\n<p>\n    <b>{{ _(\"NTN #\") }}:</b> 8015733-0 <br> \n    <b>{{ _(\"Branch\") }}:</b> {{ frappe.db.get_value('Warehouse', doc.set_warehouse, 'branch') or '' }}<br>\n    <b>{{ _(\"Receipt No\") }}:</b> {{ doc.name }}<br>\n    \n   <!-- date and time -->\n<b>{{ _(\"Date & Time\") }}:</b> \n{{ frappe.utils.formatdate(doc.posting_date, \"dd-MM-yyyy\") }} &nbsp;\n{{ frappe.utils.format_time(doc.posting_time) if doc.posting_time else \"\" }}<br>\n    \n    <b>{{ _(\"Customer\") }}:</b> {{ doc.customer_name }}<br>\n    <b>{{ _(\"Comments\") }}:</b> <br>\n</p>\n\n<div>\n    <table class=\"table table-condensed cart no-border\">\n        <thead>\n            <tr style=\"font-weight: bold;\">\n                <th width=\"50%\">{{ _(\"Product\") }}</th>\n                <th width=\"25%\" class=\"text-right\">{{ _(\"Qty\") }}</th>\n                <th width=\"25%\" class=\"text-right\">{{ _(\"Price\") }}</th>\n                <th width=\"25%\" class=\"text-right\">{{ _(\"Disc\") }}</th>\n                <th width=\"25%\" class=\"text-right\">{{ _(\"Amount\") }}</th>\n            </tr>\n        </thead>\n        <tbody>\n            {% set totals = {\"qty\": 0, \"discount\": 0.0, \"amount\": 0.0} %}\n            {%- for item in doc.items -%}\n            <tr>\n                <td>{{ item.item_name }}</td>\n                <td class=\"text-right\">{{ item.qty }}</td>\n                <td class=\"text-right\">{{ item.price_list_rate|round(2) }}</td>\n                <td class=\"text-right\">{{ item.discount_amount }}</td>\n                <td class=\"text-right\">{{ item.amount | round(2) }}</td>\n                {% if totals.update({\"qty\": totals[\"qty\"] + item.qty, \"discount\": totals[\"discount\"] + item.discount_amount, \"amount\": totals[\"amount\"] + item.amount}) %} {% endif %}\n            </tr>\n            {%- endfor -%}\n            <tr style=\"font-weight: bold;\">\n                <td></td>\n                <td class=\"text-right\">{{ totals[\"qty\"] }}</td>\n                <td class=\"text-right\"></td>\n                <td class=\"text-right\">{{ totals[\"discount\"] }}</td>\n                <td class=\"text-right\">{{ totals[\"amount\"] | round(2) }}</td>\n            </tr>\n        </tbody>\n    </table>\n</div>\n\n<div style=\"margin-top: -20px;\">\n    <table class=\"table table-condensed no-border\">\n        <tbody>\n            <!--<tr>-->\n            <!--    <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">-->\n            <!--        <b>{{ _(\"Total Excl. Tax\") }}</b>-->\n            <!--    </td>-->\n            <!--    <td class=\"text-right reduced-line-height\" style=\"width: 38%;\">-->\n            <!--        {{ doc.get_formatted(\"total\", doc) }}-->\n            <!--    </td>-->\n            <!--</tr>-->\n            \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"Discount %\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    {{ doc.additional_discount_percentage }}\n                </td>\n            </tr>\n            \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"Discount Amount(Rs)\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    {{ doc.discount_amount }}\n                </td>\n            </tr>\n            \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"Sub Total\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    {{ doc.get_formatted(\"net_total\") }}\n                </td>\n            </tr>\n    \n            {%- for row in doc.taxes -%}\n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    {{ _(\"Sales Tax\") }}\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    {{ row.get_formatted(\"tax_amount\", doc) }}\n                </td>\n            </tr>\n            {%- endfor -%}\n    \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"POS Service Fee\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    1.0\n                </td>\n            </tr>\n            \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"Total Incl. of Tax\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    {{ doc.grand_total }}\n                </td>\n            </tr>\n    \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"Paid Amount\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\">\n                    {{ doc.get_formatted(\"paid_amount\") }}\n                </td>\n            </tr>\n    \n            <tr>\n                <td class=\"text-right reduced-line-height\" style=\"width: 60%;\">\n                    <b>{{ _(\"Change Amount\") }}</b>\n                </td>\n                <td class=\"text-right reduced-line-height\"  style=\"width: 38%;\">\n                    {{ doc.get_formatted(\"change_amount\") }}\n                </td>\n            </tr>\n        </tbody>\n    </table>\n</div>\n<div style='margin-top: -30px;'>\n    <table class=\"table table-custom table-condensed cart no-border\">\n        <thead>\n            <tr>\n                <th width=\"50%\">{{ _(\"Mode Of Payment\") }}</th>\n            </tr>\n        </thead>\n        <tbody>\n            {%- for item in doc.payments -%}\n            {% if item.amount > 0 %}\n            <tr>\n                <td class=\"text-left\">{{ item.mode_of_payment.split(\" \")[0] }}</td>\n            </tr>\n            {% endif %}\n            {%- endfor -%}\n        </tbody>\n    </table>\n</div>\n<div class='greetings'>\n    Thank you for shopping at<br>Saeed Ghani\n</div>\n<div>\n    <p>For contact:</p>\n    <p style=\"font-size: 10px;\">Ph: 021-37130284 Email: customercare@saeedghani1888.com</p>\n</div>\n<div>\n    <b><p>Return & Exchange Policy :</p></b>\n    <p>{{ doc.terms or \"\" }}</p>\n    <p class=\"text-center\">\n        <ol>\n            <li>Please retain the original receipt for exchange</li>\n            <li>No cash refund policy</li>\n            <li>For exchange, all items must be in original condition</li>\n            <li>Exchange of equal value can be made within 14 days of purchase if sale receipt is provided</li>\n            <li>No exchange on discounted products</li>\n        </ol>\n    </p>\n    \n    {% if doc.barcode_ %}\n  <img \n    src=\"https://barcode.tec-it.com/barcode.ashx?data={{ doc.barcode_ }}&code=Code128&dpi=96\" \n    alt=\"Barcode\" \n    style=\"width:150px; height:40px;align:center;\"/>\n\n{% endif %}\n\n    \n    <h4><center>FBR Invoice #</center></h4>\n    <h4><center>{{ doc.fbr_invoice_no or \"\" }}</center></h4>\n    <div style=\"overflow: hidden;\">\n        <div style=\"float: left;\">\n            <img src=\"/assets/fbr_invoice/images/pos.png\" width=\"100\" alt=\"POS Image\">\n        </div>\n        <div style=\"float: right;\">\n            <img src=\"https://api.qrserver.com/v1/create-qr-code/?size=100x100&data={{ doc.fbr_invoice_no }}\" alt=\"QR Code\">\n        </div>\n    </div>\n</div>",
